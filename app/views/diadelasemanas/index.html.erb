<p id="notice"><%= notice %></p>

<h1>Diadelasemanas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diadelasemanas.each do |diadelasemana| %>
      <tr>
        <td><%= diadelasemana.nombre %></td>
        <td><%= link_to 'Show', diadelasemana %></td>
        <td><%= link_to 'Edit', edit_diadelasemana_path(diadelasemana) %></td>
        <td><%= link_to 'Destroy', diadelasemana, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diadelasemana', new_diadelasemana_path %>
